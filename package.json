{
  "name": "hello",
  "version": "1.0.0",
  "description": "<https://vuejs.org/v2/guide/#Getting-Started>",
  "main": "index.js",
  "scripts": {
    "install-pandoc-dependencies": "pip install --user pandoc-include",
    "remove-data": "sed -E -ne 's/data-v-([a-z]+)/v-\\1/;p' temp.html > index.html; rm temp.html",
    "introduction": "pandoc --from markdown --toc --bibliography=biblio.bib --citeproc --to html -s introduction.md --o temp.html",
    "the-vue-instance": "pandoc --from markdown --toc --bibliography=biblio.bib --citeproc --to html -s the-vue-instance.md --o the-vue-instance.html",
    "start": "npm run compile; npm run remove-data",
    "index": "pandoc --from markdown --bibliography=biblio.bib --citeproc  -s index.md --o index.html",
    "push": "npm start; git ci -am save && git push",
    "compile": "pandoc --toc --filter pandoc-include --from markdown --bibliography=biblio.bib --citeproc  -s index.md --o temp.html"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/crguezl/learning-vue-geting-started-guide.git"
  },
  "keywords": [],
  "author": "Casiano Rodriguez Leon <crguezl@ull.edu.es> (https://crguezl.github.io/)",
  "license": "ISC",
